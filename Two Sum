class Solution {
    public static int[] twoSum(int[] nums, int target) {
    	int[] resArray=new int[2];
        for (int i=0;i<nums.length ;i++ ) {
        	for (int j=0;j<i ;j++ ) {
        		if(nums[i]+nums[j]==target) {
                    resArray[0]=j;
                    resArray[1]=i;
        			} 
  				else ;
        		
        	}
        }return(resArray);
    }
}
